<div>
  <%= link_to 'New message', new_message_path %>
</div>
<table cellspacing="0" class="pure-table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Coordinates</th>
    </tr>
  </thead>
  <tbody>
    <%- @messages.each do |message| %>
      <tr>
        <td><%= link_to message.name, message %></td>
        <td><%= message.coordinates %></td>
      </tr>
    <% end -%>
  </tbody>
</table>
